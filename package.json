{
  "name": "@empatheticbot/time-elements",
  "version": "2.1.0",
  "description": "Date custom element that allows dynamic updates and relative dates. ",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .js,.ts && tsc --noEmit",
    "prebuild": "npm run clean && npm run lint && mkdir dist",
    "build": "tsc && rollup -c",
    "prepublishOnly": "npm run build",
    "start": "serve .",
    "update": "npm-check -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://sledsworth@github.com/empatheticbot/date-element.git"
  },
  "keywords": [
    "custom",
    "elements",
    "web",
    "components",
    "date",
    "html",
    "javascript"
  ],
  "author": "Steve Ledsworth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/empatheticbot/date-element/issues"
  },
  "homepage": "https://github.com/empatheticbot/date-element#readme",
  "devDependencies": {
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-custom-elements": "^0.0.4",
    "eslint-plugin-github": "^4.3.4",
    "eslint-plugin-import": "^2.25.2",
    "npm-check": "5.9.2",
    "prettier": "^2.4.1",
    "rollup": "^2.59.0",
    "serve": "^13.0.2",
    "typescript": "^4.4.4"
  },
  "eslintIgnore": [
    "dist/"
  ]
}